
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  presets   Json?
  createdAt DateTime @default(now())
  channels  Channel[]
}

model Channel {
  id        Int      @id @default(autoincrement())
  name      String
  presets   Json?
  createdAt DateTime @default(now())
  clientId  Int
  client    Client   @relation(fields: [clientId], references: [id])
}
